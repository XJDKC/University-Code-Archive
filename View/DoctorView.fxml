<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.DoctorViewController">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="25.0">
            <children>
                <Button fx:id="btPatientList" mnemonicParsing="false" onAction="#btPatientListOnAction"
                        prefHeight="60.0" prefWidth="200.0" stylesheets="@../Resources/DoctorView.css" text="病 人 列 表">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Button>
                <Button fx:id="btIncomeList" mnemonicParsing="false" onAction="#btIncomeListOnAction" prefHeight="60.0"
                        prefWidth="200.0" stylesheets="@../Resources/DoctorView.css" text="收 入 列 表">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Button>
                <Button fx:id="btLogout" mnemonicParsing="false" onAction="#btLogoutOnAction" prefHeight="60.0"
                        prefWidth="200.0" stylesheets="@../Resources/DoctorView.css" text="退 出 登 录">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
